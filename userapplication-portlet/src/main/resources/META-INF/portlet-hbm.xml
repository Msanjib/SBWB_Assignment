<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.lftechnology.sbwbtraining.userapplication.model.Emp" />
	<class name="com.lftechnology.sbwbtraining.userapplication.model.impl.EmpImpl" table="MO_Emp">
		<cache usage="read-write" />
		<id name="userId" type="long">
			<generator class="assigned" />
		</id>
		<property name="firstName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phoneNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="companyName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="depId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userDesc" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>