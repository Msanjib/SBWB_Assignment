<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.lftechnology.sbwbtraining.division.model.Division" />
	<class name="com.lftechnology.sbwbtraining.division.model.impl.DivisionImpl" table="MO_Division">
		<cache usage="read-write" />
		<id name="divisionId" type="long">
			<generator class="assigned" />
		</id>
		<property name="divisionName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createdBy" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createdOn" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>